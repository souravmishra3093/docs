{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "api": {
  "playground": {
    "display": "none"
  }
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "introduction",
              "authentication",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "Endpoint",
            "pages": [
              "endpoints/localpost",
              "endpoints/transpost"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://mintlify.com/docs",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://mintlify.com/community",
          "icon": "slack"
        },
        {
          "anchor": "Blog",
          "href": "https://mintlify.com/blog",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/revLogo.svg",
    "dark": "/logo/revLogo.svg"
  },
  "navbar": {
    "primary": {
      "type": "button",
      "label": "Visit RevUp",
      "href": "https://revup.reverieinc.com"
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/ReverieTech",
      "github": "https://github.com/reverieinc",
      "linkedin": "https://www.linkedin.com/company/reverie-language-technologies-pvt-ltd/posts/?feedView=all",
      "youtube": "https://www.youtube.com/@reverielanguagetechnologie2440"
    }
  }
}